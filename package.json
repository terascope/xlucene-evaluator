{
  "name": "xlucene-evaluator",
  "version": "0.2.1",
  "srcMain": "src/index.ts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "test": "tests"
  },
  "scripts": {
    "build": "node_modules/pegjs/bin/pegjs -o peg/peg_engine.js peg/lucene.grammar; tsc --project tsconfig.build.json --pretty",
    "lint": "tslint --project tsconfig.json --format verbose",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "npm run build",
    "build:watch": "npm run build --watch",
    "test": "jest",
    "test:watch": "jest --coverage=false --notify --watch --onlyChanged",
    "test:debug": "env DEBUG='*teraslice*' jest --detectOpenHandles --coverage=false --runInBand true --verbose false"
  },
  "author": "Terascope, LLC <info@terascope.io>",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@turf/turf": "^5.1.6",
    "ip-num": "^1.1.1",
    "ip6addr": "^0.2.2",
    "is-cidr": "^3.0.0",
    "latlon-geohash": "^1.1.0",
    "lodash": "^4.17.11",
    "pegjs": "^0.10.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.7",
    "@types/lodash": "^4.14.117",
    "@types/node": "^10.12.0",
    "jest": "^23.6.0",
    "jest-extended": "^0.11.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
